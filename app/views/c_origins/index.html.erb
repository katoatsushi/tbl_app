<p id="notice"><%= notice %></p>

<h1>C Origins</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @c_origins.each do |c_origin| %>
      <tr>
        <td><%= link_to 'Show', c_origin %></td>
        <td><%= link_to 'Edit', edit_c_origin_path(c_origin) %></td>
        <td><%= link_to 'Destroy', c_origin, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New C Origin', new_c_origin_path %>
